<Window x:Class="Kurs_2.UserLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kurs_2"
        mc:Ignorable="d"
        Title="UserLogin" Height="279" Width="398">
    <Grid Margin="0,0,0,-6">
        <Label Content="Логин " HorizontalAlignment="Left" Margin="59,88,0,0" VerticalAlignment="Top" Width="66"/>
        <Label Content="Пароль " HorizontalAlignment="Left" Margin="59,126,0,0" VerticalAlignment="Top" Width="66"/>
        <TextBox x:Name="AddLogin" HorizontalAlignment="Left" Margin="147,92,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>
        <TextBox x:Name="AddPassword" HorizontalAlignment="Left" Margin="147,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>
        <Button x:Name="UserLogging" Content="Войти" HorizontalAlignment="Center" Margin="0,189,0,0" VerticalAlignment="Top" Width="144" Height="37" Click="UserLogging_Click"/>

    </Grid>
</Window>
